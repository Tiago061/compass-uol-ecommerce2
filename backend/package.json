{
  "name": "backend",
  "version": "0.0.1",
  "description": "api para processo seletivo da mind group ",
  "type": "commonjs",
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/node": "18.0.1",
    "nodemon": "3.1.9",
    "ts-node": "10.8.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "dotenv": "16.0.1",
    "express": "4.21.2",
    "typeorm": "0.3.20",
    "typeorm-extension": "^3.6.3",
    "zod": "3.24.1"
  },
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node ./src/index.ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/connection.ts migration:generate ./src/migrations/default",
    "migration:run": "typeorm-ts-node-commonjs -d ./src/connection.ts migration:run"
  }
}
